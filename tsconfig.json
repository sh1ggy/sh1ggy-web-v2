{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "importsNotUsedAsValues": "remove",
    "plugins": [
      {
        "name": "@astrojs/ts-plugin"
      },
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "lib": [
      "es5",
      "es6",
      "dom",
      "dom.iterable"
    ]
  }
}